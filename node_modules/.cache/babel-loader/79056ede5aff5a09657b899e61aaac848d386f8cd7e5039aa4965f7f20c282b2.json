{"ast":null,"code":"import NavBar from \"@/components/NavBar.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"KeranjangItem\",\n  components: {\n    NavBar\n  },\n  data() {\n    return {\n      keranjangs: []\n    };\n  },\n  methods: {\n    setKeranjangs(data) {\n      this.keranjangs = data;\n    }\n  },\n  mounted() {\n    axios.get(\"http://localhost:3000/keranjangs\").then(response => this.setKeranjangs(response.data)).catch(err => console.log(err));\n  }\n};","map":{"version":3,"names":["NavBar","axios","name","components","data","keranjangs","methods","setKeranjangs","mounted","get","then","response","catch","err","console","log"],"sources":["src/views/Keranjang.vue"],"sourcesContent":["<template>\r\n  <div class=\"keranjang\">\r\n    <NavBar />\r\n    <div class=\"container\">\r\n      <div class=\"row mt-4\">\r\n        <div class=\"col\">\r\n          <h2><strong>Keranjang</strong></h2>\r\n        </div>\r\n      </div>\r\n      <div class=\"table-responsive mt-4\">\r\n        <table class=\"table\">\r\n          <thead>\r\n            <tr>\r\n              <th scope=\"col\">#</th>\r\n              <th scope=\"col\">Foto</th>\r\n              <th scope=\"col\">Makanan</th>\r\n              <th scope=\"col\">Keterangan</th>\r\n              <th scope=\"col\">Jumlah</th>\r\n              <th scope=\"col\">Harga</th>\r\n              <th scope=\"col\">Total Harga</th>\r\n              <th scope=\"col\">Hapus</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"(keranjang, index) in keranjangs\" :key=\"keranjang.id\">\r\n              <th scope=\"row\">{{ index + 1 }}</th>\r\n              <td>\r\n                <img\r\n                  :src=\"'assets/images/' + keranjang.products.gambar\"\r\n                  alt=\"gambar foto\"\r\n                  class=\"img-fluid shadow\"\r\n                  width=\"250\"\r\n                />\r\n              </td>\r\n              <td>\r\n                <strong>{{ keranjang.products.nama }}</strong>\r\n              </td>\r\n              <td>\r\n                {{\r\n                  keranjang.products.keterangan\r\n                    ? keranjang.products.keterangan\r\n                    : \"-\"\r\n                }}\r\n              </td>\r\n              <td>{{ keranjang.jumlah_pemesanan }}</td>\r\n              <td align=\"right\">Rp. {{ keranjang.products.harga }}</td>\r\n              <td align=\"right\">\r\n                Rp. {{ keranjang.jumlah_pemesanan * keranjang.products.harga }}\r\n              </td>\r\n              <td align=\"center\"><b-icon-trash></b-icon-trash></td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavBar from \"@/components/NavBar.vue\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"KeranjangItem\",\r\n  components: {\r\n    NavBar,\r\n  },\r\n  data() {\r\n    return {\r\n      keranjangs: [],\r\n    };\r\n  },\r\n  methods: {\r\n    setKeranjangs(data) {\r\n      this.keranjangs = data;\r\n    },\r\n  },\r\n  mounted() {\r\n    axios\r\n      .get(\"http://localhost:3000/keranjangs\")\r\n      .then((response) => this.setKeranjangs(response.data))\r\n      .catch((err) => console.log(err));\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AA2DA,OAAAA,MAAA;AACA,OAAAC,KAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAH;EACA;EACAI,KAAA;IACA;MACAC,UAAA;IACA;EACA;EACAC,OAAA;IACAC,cAAAH,IAAA;MACA,KAAAC,UAAA,GAAAD,IAAA;IACA;EACA;EACAI,QAAA;IACAP,KAAA,CACAQ,GAAA,qCACAC,IAAA,CAAAC,QAAA,SAAAJ,aAAA,CAAAI,QAAA,CAAAP,IAAA,GACAQ,KAAA,CAAAC,GAAA,IAAAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}