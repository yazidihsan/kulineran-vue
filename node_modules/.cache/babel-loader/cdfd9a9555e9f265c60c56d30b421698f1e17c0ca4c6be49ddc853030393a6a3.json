v aria-label=\"breadcrumb mt-4\">\r\n        <ol class=\"breadcrumb\">\r\n          <li class=\"breadcrumb-item\">\r\n            <router-link to=\"/\" class=\"text-dark\">Home</router-link>\r\n          </li>\r\n          <li class=\"breadcrumb-item\">\r\n            <router-link to=\"/foods\" class=\"text-dark\">Foods</router-link>\r\n          </li>\r\n          <li class=\"breadcrumb-item active\" aria-current=\"page\">Food Order</li>\r\n        </ol>\r\n      </nav>\r\n\r\n      <div class=\"row mt-3\">\r\n        <div class=\"col-md-6\">\r\n          <img\r\n            :src=\"'../assets/images/' + product.gambar\"\r\n            alt=\"\"\r\n            class=\"img-fluid shadow\"\r\n          />\r\n        </div>\r\n        <div class=\"col-md-6\">\r\n          <h2>\r\n            <strong>{{ product.nama }}</strong>\r\n          </h2>\r\n          <hr />\r\n          <h4>\r\n            Harga : <strong>Rp. {{ product.harga }}</strong>\r\n          </h4>\r\n          <form class=\"mt-4\" v-on:submit.prevent>\r\n            <div class=\"form-group\">\r\n              <label for=\"jumlah_pemesanan\">Jumlah Pesan</label>\r\n              <input\r\n                type=\"number\"\r\n                class=\"form-control\"\r\n                v-model=\"pesanan.jumlah_pemesanan\"\r\n              />\r\n            </div>\r\n            <div class=\"form-group\">\r\n              <label for=\"keterangan\">Keterangan</label>\r\n              <textarea\r\n                v-model=\"pesanan.keterangan\"\r\n                class=\"form-control\"\r\n                placeholder=\"Keterangan spt: Pedes, Nasi Setengah...\"\r\n              ></textarea>\r\n            </div>\r\n            <button type=\"submit\" class=\"btn btn-success\" @click=\"pemesanan\">\r\n              <b-icon-cart></b-icon-cart> Pesan\r\n            </button>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavBar from \"@/components/NavBar.vue\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"FoodDetailItem\",\r\n  components: {\r\n    NavBar,\r\n  },\r\n  data() {\r\n    return {\r\n      product: {},\r\n      pesanan: {},\r\n    };\r\n  },\r\n  methods: {\r\n    setDetail(data) {\r\n      this.product = data;\r\n    },\r\n    pemesanan() {\r\n      if (this.pesanan.jumlah_pemesanan) {\r\n        this.pesanan.products = this.product;\r\n        axios\r\n          .post(\"http://localhost:3000/keranjangs\", this.pesanan)\r\n          .then(() => {\r\n            this.$router.push({ path: \"/keranjang\" });\r\n            this.$toast.success(\"Sukses Masuk Keranjang\", {\r\n              type: \"success\",\r\n              position: \"top-right\",\r\n              duration: 3000,\r\n              dismissible: true,\r\n            });\r\n          })\r\n          .catch((err) => console.log(err));\r\n      } else {\r\n        this.$toast.error(\"Silahkan isi jumlah pesanan\", {\r\n          type: \"error\",\r\n          position: \"top-right\",\r\n          duration: 3000,\r\n          dismissible: true,\r\n        });\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    axios\r\n      .get(\"http://localhost:3000/products/\" + this.$route.params.id)\r\n      .then((response) => this.setDetail(response.data))\r\n      .catch((error) => console.log(error));\r\n  },\r\n};\r\n</script>\r\n"],"mappings":";AAkEA,OAAAA,MAAA;AACA,OAAAC,KAAA;AACA;EACAC,IAAA;EACAC,UAAA;IACAH;EACA;EACAI,KAAA;IACA;MACAC,OAAA;MACAC,OAAA;IACA;EACA;EACAC,OAAA;IACAC,UAAAJ,IAAA;MACA,KAAAC,OAAA,GAAAD,IAAA;IACA;IACAK,UAAA;MACA,SAAAH,OAAA,CAAAI,gBAAA;QACA,KAAAJ,OAAA,CAAAK,QAAA,QAAAN,OAAA;QACAJ,KAAA,CACAW,IAAA,0CAAAN,OAAA,EACAO,IAAA;UACA,KAAAC,OAAA,CAAAC,IAAA;YAAAC,IAAA;UAAA;UACA,KAAAC,MAAA,CAAAC,OAAA;YACAC,IAAA;YACAC,QAAA;YACAC,QAAA;YACAC,WAAA;UACA;QACA,GACAC,KAAA,CAAAC,GAAA,IAAAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;MACA;QACA,KAAAP,MAAA,CAAAU,KAAA;UACAR,IAAA;UACAC,QAAA;UACAC,QAAA;UACAC,WAAA;QACA;MACA;IACA;EACA;EACAM,QAAA